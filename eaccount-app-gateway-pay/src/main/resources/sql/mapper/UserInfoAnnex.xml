<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ucsmy.eaccount.pay.dao.UserInfoAnnexDao">

    <insert id="save" parameterType="com.ucsmy.eaccount.pay.entity.UserInfoAnnex">
        insert into ec_user_info_annex(
        id,
        user_no,
        user_name,
        idcard_no,
        mobile
        )
        values(
        #{id},
        #{userNo},
        #{userName},
        #{idcardNo},
        #{mobile}
        )
    </insert>
</mapper>